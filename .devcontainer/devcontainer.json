{
	"name": "poetry3-poetry-pyenv",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"containerEnv": {
		"HYDRA_FULL_ERROR": "1"
	},
	"runArgs": [
		"--shm-size=2g",
		"--memory=16g",
		"--memory-swap=24g"
	],
	"postCreateCommand": "poetry install",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python"
			]
		}
	}
}